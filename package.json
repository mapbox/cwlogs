    "url": "https://github.com/mapbox/cwlogs/issues"
  },
  "homepage": "https://github.com/mapbox/cwlogs#readme",
  "devDependencies": {
    "aws-sdk-mock": "^1.2.0",
    "documentation": "^4.0.0-beta5",
    "eslint": "^2.11.1",
    "nyc": "^6.4.4",
    "opener": "^1.4.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "aws-sdk": "^2.3.17",
    "minimist": "^1.2.0"
